// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Trash {
  id          Int       @id @default(autoincrement())
  item        String
  type        TrashType @relation(fields: [trashTypeId], references: [id])
  note        String?
  trashTypeId Int
}

model TrashType {
  id    Int     @id @default(autoincrement())
  Trash Trash[]
  name  String
  text  String?
}
